        "updater": {
            "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEMxNDAyRUI1NjlDQThCRUYKUldUdmk4cHB0UzVBd2FFbk91T1FXSnVWdERRK09tY0dSM2tyV01Tb2pIUVM2ME5qRDJuZGMwalEK",
